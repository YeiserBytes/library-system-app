{
  "kind": "collectionType",
  "collectionName": "reservations",
  "info": {
    "singularName": "reservation",
    "pluralName": "reservations",
    "displayName": "Reservation"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "reservation_date": {
      "type": "datetime",
      "required": true
    },
    "expiration_date": {
      "type": "datetime",
      "required": true
    },
    "reservation_status": {
      "type": "enumeration",
      "required": true,
      "enum": [
        "pending",
        "fulfilled",
        "cancelled",
        "expired"
      ]
    },
    "priority": {
      "type": "integer",
      "default": 0
    },
    "notes": {
      "type": "text"
    },
    "user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "reservations"
    },
    "book": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::book.book",
      "inversedBy": "reservations"
    }
  }
}
