{
  "kind": "collectionType",
  "collectionName": "activity_logs",
  "info": {
    "singularName": "activity-log",
    "pluralName": "activity-logs",
    "displayName": "Activity-Log"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "action": {
      "type": "enumeration",
      "enum": [
        "loan_created",
        "loan_returned",
        "book_added",
        "user_registered",
        "reservation_created"
      ]
    },
    "description": {
      "type": "text",
      "required": true
    },
    "ip_address": {
      "type": "string"
    },
    "metadata": {
      "type": "json"
    },
    "user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "activity_logs"
    }
  }
}
